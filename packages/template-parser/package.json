{
  "name": "@endorphinjs/template-parser",
  "version": "0.10.0",
  "description": "Parses EndorphinJS templates to ESTree-compatible AST",
  "main": "./dist/template-parser.cjs.js",
  "module": "./dist/template-parser.es.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint ./src/**/*.ts",
    "test": "mocha",
    "clean": "rm -rf ./dist",
